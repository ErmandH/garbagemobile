{
  "expo": {
    "name": "garbagemobile",
    "slug": "garbagemobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "garbagemobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyDirGn0mYGt6ws1pBsHCnWzLv-FcgFnq4g"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.garbagemobile",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "usesCleartextTraffic": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDirGn0mYGt6ws1pBsHCnWzLv-FcgFnq4g"
        }
      },
      "manifest": {
        "application": {
          "meta-data": [
            {
              "name": "com.google.android.geo.API_KEY",
              "value": "AIzaSyDirGn0mYGt6ws1pBsHCnWzLv-FcgFnq4g"
            }
          ]
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Bu uygulama harita üzerinde konumunuzu göstermek için konum erişimi gerektirir."
        }
      ],
      [
        "expo-maps",
        {
          "googleMapsApiKey": "AIzaSyDirGn0mYGt6ws1pBsHCnWzLv-FcgFnq4g"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "52ef0a0a-5513-40ad-88e9-3b0a72a50f17"
      },
      "googleMapsApiKey": "AIzaSyDirGn0mYGt6ws1pBsHCnWzLv-FcgFnq4g"
    }
  }
}
